cpu: /sys/devices/platform/samsung/performance_level
wifi: /sys/devices/platform/samsung/rfkill/rfkill0/state
bt: /sys/devices/platform/samsung/rfkill/rfkill1/state
usb: /sys/devices/platform/samsung/usb_charge

#keyboard backlight: /sys/devices/platform/samsung/leds/samsung::kbd_backlight/brightness

